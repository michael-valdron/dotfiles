{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": ["main", "work"],
  "git-submodules": {
    "enabled": true,
    "versioning": "git",
    "fileMatch": [
      "(^|/)\\.gitmodules$"
    ]
  },
  "sbt": {
    "enabled": false
  }
  "lein-profile": {
    "managerFilePatterns": [
      "/(^|/)project\\.clj$/"
    ],
    "versioning": "maven"
  }
}
