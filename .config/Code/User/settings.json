{
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.colorTheme": "Monokai",
    "redhat.telemetry.enabled": false,
    "editor.fontSize": 15,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "git.alwaysSignOff": true,
    "extensions.ignoreRecommendations": true,
    "go.toolsManagement.autoUpdate": true,
    "nxConsole.enableTelemetry": false,
    "openapi.approvedHostnames": [
        "raw.githubusercontent.com"
    ]
    "vscode-kubernetes.minikube-path": "/bin/minikube",
    "vscode-kubernetes.kubectl-path": "/bin/minikube kubectl -- ",
    "vs-kubernetes": {
        "vscode-kubernetes.kubectl-path.linux": "~/.local/state/vs-kubernetes/tools/kubectl/kubectl",
        "vscode-kubernetes.helm-path.linux": "~/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm"
    },
    "vscode-kubernetes.helm-path": "/bin/helm"
}
